<UserControl x:Class="UserInterfaces.PasswordPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <PasswordBox HorizontalAlignment="Left" Margin="95,25,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="120" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="95,54,0,0" Name="passwordBox2" VerticalAlignment="Top" Width="120" PasswordChar="-" />
        <TextBlock HorizontalAlignment="Left" Margin="35,29,0,0" Name="textBlock1" Text="Password" VerticalAlignment="Top" />
        <TextBlock HorizontalAlignment="Left" Margin="35,58,0,0" Name="textBlock2" Text="Conferma" VerticalAlignment="Top" />
    </Grid>
</UserControl>
